{
  "name": "cshiftfgo",
  "displayName": "CShiftFGo",
  "description": "按住cmd/ctrl+shift+f键可以跳转",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "File Shortcuts",
      "properties": {
        "fileShortcuts.showInEditor": {
          "type": "boolean",
          "default": true
        }
     }
    },
    "commands": [
      {
        "command": "extension.showShortcutModal",
        "title": "Show File Shortcuts"
      },
      {
        "command": "cshiftfgo.onButtonClick",
        "title": "文件按钮点击"
      },
      {
        "command": "file-shortcuts.activate",
        "title": "Activate File Shortcuts"
      },
      {
        "command": "file-shortcuts.onKeyPress",
        "title": "Handle Key Press"
      },
      {
        "command": "fileShortcuts.openFile",
        "title": "Open File"
      },
      {
        "command": "fileShortcuts.refresh",
        "title": "Refresh File Shortcuts"
      }
    ],
    "keybindings": [
      {
        "command": "extension.showShortcutModal",
        "key": "f1",
        "mac": "f1"
      },
      {
        "command": "file-shortcuts.onKeyPress",
        "key": "a",
        "args": "A",
        "when": "explorerViewletVisible && cshiftfgo.shortcutsActive "
      },
      {
        "command": "file-shortcuts.onKeyPress",
        "key": "b",
        "args": "B",
        "when": "explorerViewletVisible && cshiftfgo.shortcutsActive "
      },
      {
        "command": "file-shortcuts.onKeyPress",
        "key": "c",
        "args": "C",
        "when": "explorerViewletVisible && cshiftfgo.shortcutsActive "
      },
      {
        "command": "file-shortcuts.onKeyPress",
        "key": "d",
        "args": "D",
        "when": "explorerViewletVisible && cshiftfgo.shortcutsActive "
      },
      {
        "command": "file-shortcuts.onKeyPress",
        "key": "e",
        "args": "E",
