{
  "name": "cshiftfgo",
  "displayName": "CShiftFGo",
  "description": "按住cmd/ctrl+shift+f键可以跳转",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      
      {
        "command": "cshiftfgo.onKeyPress",
        "title": "键盘按键被按下"
      },
      
      {
        "command": "cshiftfgo.closeCshiftgo",
        "title": "关闭cshiftgo(快捷键插件)"
      }
    ],
    "keybindings": [
      {
        "key": "space x",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "variables.collapse"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space a",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "workbench.debug.viewlet.action.addWatchExpression"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space r",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "workbench.debug.viewlet.action.removeAllWatchExpressions"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space c",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "watch.collapse"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space d",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "extension.js-debug.disableSourceMapStepping"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive && !jsDebugIsMapSteppingDisabled && debugType == 'node-terminal'  || !jsDebugIsMapSteppingDisabled && debugType == 'pwa-chrome'  || !jsDebugIsMapSteppingDisabled && debugType == 'pwa-extensionHost' || !jsDebugIsMapSteppingDisabled && debugType == 'pwa-msedge'  || !jsDebugIsMapSteppingDisabled && debugType == 'pwa-node'"
      },
      {
        "key": "space f",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "extension.js-debug.enableSourceMapStepping"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive && jsDebugIsMapSteppingDisabled && debugType == 'node-terminal'  || jsDebugIsMapSteppingDisabled && debugType == 'pwa-chrome'  || jsDebugIsMapSteppingDisabled &&  debugType == 'pwa-extensionHost'  || jsDebugIsMapSteppingDisabled && debugType == 'pwa-msedge' || jsDebugIsMapSteppingDisabled && debugType == 'pwa-node'"
      },
      {
        "key": "space b",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "callStack.collapse"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space n",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "loadedScripts.collapse"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space s",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "workbench.debug.viewlet.action.addFunctionBreakpointAction"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space g",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "workbench.debug.viewlet.action.toggleBreakpointsActivatedAction"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "key": "space t",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "workbench.debug.viewlet.action.removeAllBreakpoints"
            },
            {
              "command": "cshiftfgo.closeCshiftgo"
            }
          ]
        },
        "when": "cshiftfgo.shortcutsActive"
      },
      {
        "command": "cshiftfgo.showShortcutOverlay_explorer",
        "key": "alt+M",
        "mac": "alt+M",
        "when": "activeViewlet == 'workbench.view.explorer' && viewContainer.workbench.view.explorer.enabled"
      },
      {
        "command": "cshiftfgo.showShortcutOverlay_search",
        "key": "alt+M",
        "mac": "alt+M",
        "when": "activeViewlet == 'workbench.view.search' && workbench.view.search.active"
      },
      {
        "command": "cshiftfgo.showShortcutOverlay_scm",
        "key": "alt+M",
        "mac": "alt+M",
        "when": "activeViewlet == 'workbench.view.scm' && workbench.scm.active"
      },
      {
        "command": "cshiftfgo.showShortcutOverlay_debug",
        "key": "alt+M",
        "mac": "alt+M",
        "when": "activeViewlet == 'workbench.view.debug' && viewContainer.workbench.view.debug.enabled"
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "a",
        "args": "A",
        "when": " cshiftfgo.shortcutsActive "
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "b",
        "args": "B",
        "when": " cshiftfgo.shortcutsActive "
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "c",
        "args": "C",
        "when": " cshiftfgo.shortcutsActive "
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "d",
        "args": "D",
        "when": " cshiftfgo.shortcutsActive "
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "e",
        "args": "E",
        "when": " cshiftfgo.shortcutsActive "
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "f",
        "args": "F",
        "when": " cshiftfgo.shortcutsActive "
      },
      {
        "command": "cshiftfgo.onKeyPress",
        "key": "g",
        "args": "G",
